generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Extension {
  id String @id @default(uuid())
  name String
  description String?
  active Boolean @default(false)
  avatarURL String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("extensions")
}